{
  "name": "inu",
  "version": "3.1.4",
  "description": "composable and modular unidirectional user interfaces",
  "main": "index.js",
  "scripts": {
    "start": "budo examples:bundle.js -d examples --live -- -d",
    "test:deps": "dependency-check . && dependency-check . --extra --no-dev -i es2040",
    "test:lint": "standard",
    "test:node": "NODE_ENV=test tape test/*.js",
    "test:coverage": "NODE_ENV=test nyc npm run test:node",
    "test:coverage:report": "nyc report --reporter=lcov npm run test:node",
    "test": "npm-run-all -s test:node test:lint test:deps",
    "docs:disc": "browserify index.js -g envify -g uglifyify --full-paths | discify > assets/disc.html",
    "docs": "npm-run-all -p docs:*",
    "bundle": "browserify examples -g envify -g uglifyify",
    "build": "npm run --silent bundle -- -o examples/bundle.js",
    "push": "gh-pages -d examples",
    "deploy": "npm-run-all -s build push"
  },
  "browserify": {
    "transform": [
      "es2040"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahdinosaur/inu.git"
  },
  "keywords": [],
  "author": "Mikey <michael.williams@enspiral.com> (http://dinosaur.is)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ahdinosaur/inu/issues"
  },
  "homepage": "https://github.com/ahdinosaur/inu#readme",
  "devDependencies": {
    "browserify": "^13.0.0",
    "budo": "^8.1.0",
    "dependency-check": "^2.5.1",
    "depject": "^3.1.5",
    "disc": "^1.3.2",
    "envify": "^3.4.0",
    "gh-pages": "^0.11.0",
    "inu-engine": "github:ahdinosaur/inu-engine",
    "npm-run-all": "^1.6.0",
    "standard": "^8.2.0",
    "tape": "^4.5.1",
    "uglifyify": "^3.0.1",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "@f/set-prop": "^1.0.0",
    "es2040": "^1.2.4",
    "inu-multi": "^1.0.4",
    "libnested": "^1.1.0",
    "object-assign": "^4.1.1",
    "pull-many": "^1.0.8",
    "pull-stream": "^3.5.0",
    "yo-yo": "^1.3.2"
  }
}
