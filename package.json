{
  "name": "inu",
  "version": "1.0.0",
  "description": "composable user interface state and effects manager",
  "main": "index.js",
  "scripts": {
    "test": "tape test.js",
    "start": "budo example:bundle.js -d example --live -- -d",
    "bundle": "browserify example -g envify -g uglifyify",
    "build": "npm run --silent bundle -- -o example/bundle.js",
    "disc": "browserify index.js -g envify -g uglifyify --full-paths | discify --open",
    "push": "gh-pages -d example",
    "deploy": "npm-run-all -s build push"
  },
  "browserify": {
    "transform": []
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahdinosaur/inu.git"
  },
  "keywords": [],
  "author": "Mikey <michael.williams@enspiral.com> (http://dinosaur.is)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ahdinosaur/inu/issues"
  },
  "homepage": "https://github.com/ahdinosaur/inu#readme",
  "devDependencies": {
    "browserify": "^13.0.0",
    "budo": "^8.1.0",
    "disc": "^1.3.2",
    "envify": "^3.4.0",
    "gh-pages": "^0.11.0",
    "npm-run-all": "^1.6.0",
    "tape": "^4.5.1",
    "uglifyify": "^3.0.1",
    "watchify": "^3.7.0",
    "yo-yo": "^1.2.0"
  },
  "dependencies": {
    "pull-stream": "^3.3.2",
    "push-stream": "^2.0.2"
  }
}
